#if HasMatlab
EXTRA_DEFINES = -DHAS_MATLAB -DV5_COMPAT
#endif

CODE = mat2exo

SRC = mat2exo.c

OBJS = $(SRC:.c=.o)

INCLUDES = MatlabIncludes

LIBS = $(SUPLIB) $(EXOIIV2C) $(NETCDF) $(MATLABLIBRARIES) -lm

NormalCTarget($(CODE),$(OBJS),NullParameter,NullParameter,$(LIBS))

InstallTarget(install,$(CODE),$(INSTBINFLAGS),$(BINDIR))
