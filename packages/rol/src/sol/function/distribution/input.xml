<ParameterList name="Inputs">
  <!-- ===========  STOCHASTIC OPTIMIZATION LIBRARY SUBLIST  =========== -->
  <ParameterList name="SOL">
    <!-- ===========  1D DISTRIBUTION SUBLIST  =========== -->
    <ParameterList name="Distribution">
      <Parameter name="Name" type="string" value="Gaussian" />
      <!-- ===========  ARCSINE DISTRIBUTION SUBLIST  =========== -->
      <ParameterList name="Arcsine">
        <Parameter name="Lower Bound" type="double" value="0." />
        <Parameter name="Upper Bound" type="double" value="1." />
      </ParameterList>
      <!-- ===========  BETA DISTRIBUTION SUBLIST  =========== -->
      <ParameterList name="Beta">
        <Parameter name="Shape 1" type="double" value="2." />
        <Parameter name="Shape 2" type="double" value="2." />
      </ParameterList>
      <!-- ===========  CAUCHY DISTRIBUTION SUBLIST  =========== -->
      <ParameterList name="Cauchy">
        <Parameter name="Location" type="double" value="0." />
        <Parameter name="Scale"    type="double" value="1." />
      </ParameterList>
      <!-- ===========  DIRAC DISTRIBUTION SUBLIST  =========== -->
      <ParameterList name="Dirac">
        <Parameter name="Location" type="double" value="0." />
      </ParameterList>
      <!-- ===========  EXPONENTIAL DISTRIBUTION SUBLIST  =========== -->
      <ParameterList name="Exponential">
        <Parameter name="Location" type="double" value="0." />
        <Parameter name="Scale"    type="double" value="1." />
      </ParameterList>
      <!-- ===========  GAMMA DISTRIBUTION SUBLIST  =========== -->
      <ParameterList name="Gamma">
        <Parameter name="Shape" type="double" value="1." />
        <Parameter name="Scale" type="double" value="1." />
      </ParameterList>
      <!-- ===========  GAUSSIAN DISTRIBUTION SUBLIST  =========== -->
      <ParameterList name="Gaussian">
        <Parameter name="Mean"     type="double" value="0." />
        <Parameter name="Variance" type="double" value="1." />
      </ParameterList>
      <!-- ===========  KUMARASWAMY DISTRIBUTION SUBLIST  =========== -->
      <ParameterList name="Kumaraswamy">
        <Parameter name="Lower Bound" type="double" value="0." />
        <Parameter name="Upper Bound" type="double" value="1." />
        <Parameter name="Exponent 1" type="double" value="0.5" />
        <Parameter name="Exponent 2" type="double" value="0.5" />
      </ParameterList>
      <!-- ===========  LAPLACE DISTRIBUTION SUBLIST  =========== -->
      <ParameterList name="Laplace">
        <Parameter name="Mean"  type="double" value="0." />
        <Parameter name="Scale" type="double" value="1." />
      </ParameterList>
      <!-- ===========  LOGISTIC DISTRIBUTION SUBLIST  =========== -->
      <ParameterList name="Logistic">
        <Parameter name="Mean"  type="double" value="0." />
        <Parameter name="Scale" type="double" value="1." />
      </ParameterList>
      <!-- ===========  PARABOLIC DISTRIBUTION SUBLIST  =========== -->
      <ParameterList name="Parabolic">
        <Parameter name="Lower Bound" type="double" value="0." />
        <Parameter name="Upper Bound" type="double" value="1." />
      </ParameterList>
      <!-- ===========  RAISED COSINE DISTRIBUTION SUBLIST  =========== -->
      <ParameterList name="Raised Cosine">
        <Parameter name="Mean"  type="double" value="0.5" />
        <Parameter name="Scale" type="double" value="0.5" />
      </ParameterList>
      <!-- ===========  SMALE DISTRIBUTION SUBLIST  =========== -->
      <ParameterList name="Smale">
        <Parameter name="Lower Bound" type="double" value="0." />
        <Parameter name="Upper Bound" type="double" value="1." />
      </ParameterList>
      <!-- ===========  TRIANGLE DISTRIBUTION SUBLIST  =========== -->
      <ParameterList name="Triangle">
        <Parameter name="Lower Bound"   type="double" value="0."  />
        <Parameter name="Peak Location" type="double" value="0.5" />
        <Parameter name="Upper Bound"   type="double" value="1."  />
      </ParameterList>
      <!-- ===========  TRUNCATED EXPONENTIAL DISTRIBUTION SUBLIST  =========== -->
      <ParameterList name="Truncated Exponential">
        <Parameter name="Lower Bound" type="double" value="0." />
        <Parameter name="Upper Bound" type="double" value="1." />
        <Parameter name="Scale"       type="double" value="1." />
      </ParameterList>
      <!-- ===========  TRUNCATED GAUSSIAN DISTRIBUTION SUBLIST  =========== -->
      <ParameterList name="Truncated Gaussian">
        <Parameter name="Lower Bound"        type="double" value="-1." />
        <Parameter name="Upper Bound"        type="double" value="1."  />
        <Parameter name="Mean"               type="double" value="0."  />
        <Parameter name="Standard Deviation" type="double" value="1."  />
      </ParameterList>
    </ParameterList>
  </ParameterList>
</ParameterList>
