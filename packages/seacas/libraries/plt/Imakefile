SRCSf = ${SRCSF:.F=.f}
OBJSF = ${SRCSF:.F=.o}
OBJS1 = ${SRCS1:.f=.o}
OBJS2 = ${SRCS2:.f=.o}
OBJSf = $(OBJS1) $(OBJS2)

NormalLibraryTarget2($(LIB),$(OBJSF),$(OBJSf))

InstallLibrary($(LIB),$(LIBDIR))

#ifdef CrayArchitecture
pltply.o: pltply.f
	cf77 -O0 -c pltply.f
#endif

CleanupCapF($(SRCSf))
