<ParameterList>
  <Parameter name="Network Coupling Parameter Indices" type="Array(int)" 
	     value="{ 0, 0 }"/>
  <Parameter name="Network Coupling Response Indices" type="Array(int)" 
	     value="{ 0, 0 }"/>
  <Parameter name="Stochastic" type="bool" value="1"/>
  <ParameterList name="Dimension Reduction">
    <Parameter name="Reduce Dimension" type="bool" value="0"/>
    <Parameter name="Reduced Basis Method" type="string"
	       value="Product Lanczos"/>
    <Parameter name="Reduced Order" type="int" value="-1"/>
  </ParameterList>
  <Parameter name="Solver Type" type="string" value="NOX"/>
  <Parameter name="Print Convergence Stats" type="bool" value="0"/>
  <ParameterList name="NOX">
    <Parameter name="Reset Initial Guess" type="bool" value="1"/>
    <ParameterList name="Direction">
      <Parameter name="Method" type="string" value="Newton"/>
      <ParameterList name="Newton">
        <Parameter name="Forcing Term Method" type="string" value="Constant"/>
        <Parameter name="Rescue Bad Newton Solve" type="bool" value="1"/>
	<ParameterList name="Linear Solver">
	  <Parameter name="Write Linear System" type="bool" value="true"/>
	</ParameterList>
        <ParameterList name="Stratimikos Linear Solver">
          <ParameterList name="NOX Stratimikos Options">
          </ParameterList>
          <ParameterList name="Stratimikos">
            <Parameter name="Linear Solver Type" type="string" value="AztecOO"/>
            <ParameterList name="Linear Solver Types">
              <ParameterList name="AztecOO">
                <ParameterList name="Forward Solve"> 
                  <ParameterList name="AztecOO Settings">
                    <Parameter name="Aztec Solver" type="string" value="GMRES"/>
                    <Parameter name="Convergence Test" type="string" value="r0"/>
                    <Parameter name="Size of Krylov Subspace" type="int" value="200"/>
                    <Parameter name="Output Frequency" type="int" value="10"/>
                  </ParameterList>
                  <Parameter name="Max Iterations" type="int" value="200"/>
                  <Parameter name="Tolerance" type="double" value="1e-5"/>
                </ParameterList>
              </ParameterList>
            </ParameterList>
            <Parameter name="Preconditioner Type" type="string" value="None"/>
            <ParameterList name="Preconditioner Types">
              <ParameterList name="Ifpack">
                <Parameter name="Overlap" type="int" value="1"/>
                <Parameter name="Prec Type" type="string" value="ILU"/>
                <ParameterList name="Ifpack Settings">
                  <Parameter name="fact: drop tolerance" type="double" value="0"/>
                  <Parameter name="fact: ilut level-of-fill" type="double" value="1"/>
                  <Parameter name="fact: level-of-fill" type="int" value="1"/>
                </ParameterList>
              </ParameterList>
            </ParameterList>
          </ParameterList>
        </ParameterList>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Line Search">
      <ParameterList name="Full Step">
        <Parameter name="Full Step" type="double" value="1"/>
      </ParameterList>
      <Parameter name="Method" type="string" value="Full Step"/>
    </ParameterList>
    <Parameter name="Nonlinear Solver" type="string" value="Line Search Based"/>
    <ParameterList name="Printing">
      <Parameter name="Output Precision" type="int" value="3"/>
      <Parameter name="Output Processor" type="int" value="0"/>
      <ParameterList name="Output Information">
        <Parameter name="Error" type="bool" value="1"/>
        <Parameter name="Warning" type="bool" value="1"/>
        <Parameter name="Outer Iteration" type="bool" value="1"/>
        <Parameter name="Parameters" type="bool" value="0"/>
        <Parameter name="Details" type="bool" value="0"/>
        <Parameter name="Linear Solver Details" type="bool" value="1"/>
        <Parameter name="Stepper Iteration" type="bool" value="1"/>
        <Parameter name="Stepper Details" type="bool" value="1"/>
        <Parameter name="Stepper Parameters" type="bool" value="0"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Solver Options">
      <Parameter name="Status Test Check Type" type="string" value="Minimal"/>
    </ParameterList>
    <ParameterList name="Status Tests">
      <Parameter name="Test Type" type="string" value="Combo"/>
      <Parameter name="Combo Type" type="string" value="OR"/>
      <Parameter name="Number of Tests" type="int" value="2"/>
      <ParameterList name="Test 0">
        <Parameter name="Test Type" type="string" value="NormF"/>
        <Parameter name="Tolerance" type="double" value="1.0e-8"/>
      </ParameterList>
      <ParameterList name="Test 1">
        <Parameter name="Test Type" type="string" value="MaxIters"/>
        <Parameter name="Maximum Iterations" type="int" value="10"/>
      </ParameterList>
    </ParameterList>
  </ParameterList>

  <ParameterList name="Stochastic Galerkin">
    <Parameter name="SG Method" type="string" value="Direct"/>
    <Parameter name="Number of Spatial Processors" type="int" value="1"/>
    <Parameter name="Triple Product Size" type="string" value="Full"/>
    <ParameterList name="Basis">
      <Parameter name="Isotropic" type="bool" value="true"/>
      <Parameter name="Type" type="string" value="Legendre"/>
      <Parameter name="Dimension" type="int" value="2"/>
      <Parameter name="Order" type="int" value="3"/>
      <Parameter name="Normalize" type="bool" value="false"/>
      <Parameter name="Cijk Drop Tolerance" type="double" value="1e-12"/>
    </ParameterList>
    <ParameterList name="SG Parameters">
      <Parameter name="Number of SG Parameter Vectors" type="int" value="2"/>
      <ParameterList name="SG Parameter Vector 0">
	<Parameter name="Parameter Vector Index" type="int" value="0"/>
	<Parameter name="Parameter 0 Initial Expansion Coefficients" type="Array(double)" value="{ 2.0, 0.1, 0.0 }"/>
      </ParameterList>
      <ParameterList name="SG Parameter Vector 1">
	<Parameter name="Parameter Vector Index" type="int" value="1"/>
	<Parameter name="Parameter 0 Initial Expansion Coefficients" type="Array(double)" value="{ 2.0, 0.0, 0.1 }"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Expansion">
      <Parameter name="Type" type="string" value="Quadrature"/>
    </ParameterList>
    <ParameterList name="SG Solver Parameters">
      <Parameter name="Jacobian Expansion Type" type="string" value="Full"/>
      <Parameter name="Parameter Expansion Type" type="string" value="Full"/>
      <Parameter name="SG Solver Algorithm" type="string" value="Krylov"/>
      <ParameterList name="SG Operator">
	<!--Parameter name="Operator Method" type="string" value="Matrix Free"/-->
	<Parameter name="Operator Method" type="string" value="Fully Assembled"/>
      </ParameterList>
      <ParameterList name="SG Preconditioner">
	<Parameter name="Preconditioner Method" type="string" value="Approximate Gauss-Seidel"/>
	<Parameter name="Mean Preconditioner Type" type="string" value="Ifpack"/>
	<ParameterList name="Mean Preconditioner Parameters"/>
      </ParameterList>
    </ParameterList>
  </ParameterList>

  <ParameterList name="Regression Tests">
    <Parameter  name="Response 0 Mean Test Values" type="Array(double)" value="{-2.41443477761}"/>
    <Parameter  name="Response 0 Standard Deviation Test Values" type="Array(double)" value="{0.0603614149637}"/>
    <Parameter  name="Response 1 Mean Test Values" type="Array(double)" value="{-2.41443477761}"/>
    <Parameter  name="Response 1 Standard Deviation Test Values" type="Array(double)" value="{0.0603614149637}"/>
    <Parameter  name="Response 2 Mean Test Values" type="Array(double)" value="{-0.414434777609, -0.414434777609}"/>
    <Parameter  name="Response 2 Standard Deviation Test Values" type="Array(double)" value="{0.0103693702038, 0.0103693702038}"/>
    <Parameter  name="Relative Tolerance" type="double" value="1.0e-3"/>
    <Parameter  name="Absolute Tolerance" type="double" value="1.0e-8"/>
  </ParameterList>


</ParameterList>
